<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- Hotkey menu -->
    <system:String x:Key="MenuImportHotkeys">Import hotkeys</system:String>
    <system:String x:Key="MenuExportHotkeys">Export hotkeys</system:String>
    
    <!-- Option menu -->
    <system:String x:Key="MenuOptionsHeader">Options</system:String>
    <system:String x:Key="MenuInitHidden">Init hidden</system:String>
    <system:String x:Key="MenuWindowsStart">Start with Windows</system:String>
    
    <!-- Buttons -->
    <system:String x:Key="AddButton">Add</system:String>
    <system:String x:Key="DeleteButton">Delete</system:String>
    <system:String x:Key="EditButton">Edit</system:String>
    <system:String x:Key="OptionButton">Options</system:String>
    
</ResourceDictionary>