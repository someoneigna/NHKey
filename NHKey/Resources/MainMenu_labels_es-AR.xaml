<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- Hotkey menu -->
    <system:String x:Key="MenuImportHotkeys">Importar hotkeys</system:String>
    <system:String x:Key="MenuExportHotkeys">Exportar hotkeys</system:String>

    <!-- Option menu -->
    <system:String x:Key="MenuOptionsHeader">Opciones</system:String>
    <system:String x:Key="MenuInitHidden">Iniciar minimizado</system:String>
    <system:String x:Key="MenuWindowsStartup">Iniciar con Windows</system:String>

    <!-- Buttons -->
    <system:String x:Key="AddButton">Agregar</system:String>
    <system:String x:Key="DeleteButton">Eliminar</system:String>
    <system:String x:Key="EditButton">Editar</system:String>
    <system:String x:Key="OptionButton">Opciones</system:String>
    
    <!-- Hotkey labels -->
    <system:String x:Key="OrphanedHotkeyLabel">Invalida</system:String>
    
    <!--Notify Icon labels -->
    <system:String x:Key="NotifyIconBalloonTipTitle">Minimizado</system:String>
    <system:String x:Key="NotifyIconBalloonTipText">Para reabrir haga doble clic.</system:String>

</ResourceDictionary>