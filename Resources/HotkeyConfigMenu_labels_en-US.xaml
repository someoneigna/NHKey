<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <system:String x:Key="HotkeyConfigTitle">Hotkey creation</system:String>
    
    <!-- Labels -->
    <system:String x:Key="NameLabel">Name</system:String>
    <system:String x:Key="ProgramLabel">Program</system:String>
    <system:String x:Key="ParameterLabel">Parameters</system:String>
    <system:String x:Key="ParameterFieldTooltip">The parameters to pass to the program.</system:String>
    <system:String x:Key="CombinationLabel">Combination</system:String>
    <system:String x:Key="CombinationFieldTooltip">The key combination for the hotkey.</system:String>

    <!-- Buttons -->
    <system:String x:Key="SearchButton">Search</system:String>
    <system:String x:Key="SearchButtonTooltip">Opens a dialog for program selection.</system:String>
    <system:String x:Key="SaveButton">Save</system:String>
    <system:String x:Key="CancelButton">Cancel</system:String>
    
    <!-- Messages -->
    <system:String x:Key="UnfilledFieldsMessage">You must fill all the fields before continuing.</system:String>
    <system:String x:Key="UnfilledFieldsMsgBoxTitle">Invalid field state</system:String>

    <!-- Dialogs -->
    <system:String x:Key="SearchProgramTitle">Select a program</system:String>


</ResourceDictionary>